<h1>users</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
       {{#each users}}
    <tr>
       
      <th scope="row">1</th>
      <td>{{this.Name}}</td>
      <td>{{this.Email}}</td>
      <td><a class="btn btn-secondary" href="/view-detail/{{this._id}}"> view for order</a> </td>
    </tr>
    {{/each}}
    
  </tbody>
</table>